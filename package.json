{
    "name": "movie-database",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "config": "ts-node ./setenv.ts",
        "start": "node server.js",
        "local": "npm run config -- --environment=dev && ng serve",
        "build": "npm run config -- --environment=prod && ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng npm run config -- --environment=prod && test",
        "heroku-postbuild": "npm run config -- --environment=prod && ng build",
        "prepare": "husky install",
        "format": "pretty-quick --staged"
    },
    "engines": {
        "node": "16.15.0",
        "npm": "8.5.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "~13.3.7",
        "@angular/animations": "~13.3.0",
        "@angular/cdk": "~13.3.0",
        "@angular/cli": "^14.0.0",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/compiler-cli": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/forms": "~13.3.0",
        "@angular/material": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "dotenv": "^16.0.1",
        "express": "^4.18.1",
        "path": "^0.12.7",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "typescript": "~4.6.2",
        "yargs": "^17.5.1",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.7",
        "@angular/cli": "^14.0.0",
        "@angular/compiler-cli": "~13.3.0",
        "@commitlint/cli": "^17.0.2",
        "@commitlint/config-conventional": "^17.0.2",
        "@types/jasmine": "~3.10.0",
        "@types/node": "^12.11.1",
        "autoprefixer": "^10.4.7",
        "husky": "^8.0.1",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "postcss": "^8.4.14",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "tailwindcss": "^3.1.2",
        "typescript": "~4.6.2"
    }
}
